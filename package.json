{
  "name": "wasmer-ocaml",
  "version": "0.0.0",
  "description": "My Project",
  "esy": {
    "build": "dune build -p <PACKAGE_NAME>",
    "buildDev": "pesy",
    "release": { "releasedBinaries": [ "WasmerOcamlApp.exe" ] }
  },
  "buildDirs": {
    "test": {
      "require": [ "wasmer-ocaml.lib" ],
      "main": "TestWasmerOcaml",
      "name": "TestWasmerOcaml.exe"
    },
    "library": { "name": "wasmer-ocaml.lib", "namespace": "WasmerOcaml" },
    "executable": {
      "require": [ "wasmer-ocaml.lib" ],
      "main": "WasmerOcamlApp",
      "name": "WasmerOcamlApp.exe"
    }
  },
  "scripts": {
    "pesy": "bash -c 'env PESY_MODE=update pesy'",
    "test": "esy x TestWasmerOcaml.exe",
    "start": "nodemon --delay '200ms' --watch ./ --watch ppx/ --watch vendor/ -e re,ml,sql --exec 'build.sh'"
  },
  "dependencies": {
    "@esy-ocaml/reason": "*",
    "@opam/ctypes": "0.17.1",
    "@opam/dune": ">=1.6.0",
    "ocaml": "^4.10.0"
  },
  "devDependencies": {
    "@opam/merlin": "3.3.4",
    "nodemon": "*",
    "ocaml": "4.10.0",
    "refmterr": "*",
    "pesy": "*",
    "@opam/merlin": "*"
  }
}
